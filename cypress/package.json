{
  "dependencies": {
    "@aws-sdk/client-s3": "^3.620.1",
    "@aws-sdk/client-secrets-manager": "^3.620.1",
    "cypress": "^12.7.0",
    "fs-extra": "^10.1.0",
    "jsonwebtoken": "^8.5.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-cloudwatch": "^6.1.1",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^16.18.4",
    "@types/uuid": "^9.0.1",
    "@types/yargs": "^17.0.15",
    "esbuild": "^0.17.12",
    "typescript": "~5.1.6"
